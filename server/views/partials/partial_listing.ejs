<div id="all">
        <div id="heading-breadcrumbs">
            <div class="container">
                <div class="row">
                    <div class="col-md-7">
                        <h1>Listing Page</h1>
                    </div>
                </div>
            </div>
        </div>

        <div id="content">
            <div class="container">

                <section>

                    <div class="row">
                        <div class="col-md-12">
                            <div class="heading">
                                <h2>Property List</h2>
                            </div>

                            <p class="lead">Find below the list of properties</p>
                        </div>
                    </div>

                    <div class="row portfolio">

                    <% listings.forEach(function(listing) { %>

                        <div class="col-sm-6 col-md-3">
                            <div class="box-image">
                                <div class="image">
                                    <img src= <%= listing.photo_url %> alt="" class="img-responsive">
                                </div>
                                <div class="bg"></div>
                                <div class="name">
                                    <h3><a href="portfolio-detail.html"> <%= listing.address %> </a></h3> 
                                </div>
                                <div class="text">
                                    <p class="hidden-sm hidden-lg hidden-md"> <%= listing.description %> </p>
                                    <p class="buttons">
                                        <a href="portfolio-detail.html" class="btn btn-template-transparent-primary">View</a>
                                        <a href="#" class="btn btn-template-transparent-primary">Buy Units</a>
                                    </p>
                                </div>
                            </div>
                            <!-- /.box-image -->
                        </div>
                    <% }); %>

                    </div>

                </section>

            </div>
            <!-- /.container -->

            
            <!-- /.bar -->

            


        </div>
        <!-- /#content -->

